def fun1(){
  println"1st" 
}
def fun2(int a=40, int b=30){

  println"${a+b}" 
}
pipeline {
  agent any 
  stages {
    stage('working with switch statemetn') {
      steps {
      script {
        fun1()

        }
      }
      }
    }
  stages {
    stage('working with switch statemetn') {
      steps {
      script {
        fun1()
        fun2(10,20)
        fun2()
        fun2(b=100,a=2000)
        }
      }
      }
    }
  }

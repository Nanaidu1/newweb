pipeline {
  agent any 
  stages {
    stage('working with switch statement') {
      steps {
      script {
        def myval1 = input message: 'enter val1 value', parameters: [string(defaultValue: '20', description: '', name: 'val1', trim: false)]
        switch(myval1.toInteger()) {
          case 1:
            println "I am 1"
            break
          case 2:
            println "I am 2"
			break
          default:
            println "Print either 1 or 2"
        }
      }
      }
    }
  }
}
